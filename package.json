{
  "name": "auto-interpolation",
  "displayName": "Auto Interpolation to Ruby",
  "description": "Automatically converts quotes to help interpolation",
  "icon": "images/icon.png",
  "homepage": "https://github.com/LukasPol/vscode-auto-interpolation",
  "repository": "https://github.com/LukasPol/vscode-auto-interpolation",
  "bugs": {
    "url": "https://github.com/LukasPol/vscode-auto-interpolation/issues"
  },
  "publisher": "LukasPol",
  "license": "MIT",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:ruby"
  ],
  "main": "./out/extension.js",
  "contributes": {},
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
